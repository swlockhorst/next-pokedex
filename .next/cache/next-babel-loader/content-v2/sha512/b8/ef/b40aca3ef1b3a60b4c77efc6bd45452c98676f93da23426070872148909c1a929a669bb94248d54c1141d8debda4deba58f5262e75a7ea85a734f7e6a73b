\n\n  return (\n    <div className=\"container\">\n      <Head>\n        <title>Pokedex</title>\n        <link rel=\"icon\" href=\"/favicon.ico\" />\n      </Head>\n\n      <main>\n        <Container className=\"p-5\">\n          <Pagination>\n            <Pagination.First\n              disabled={active === 1}\n              onClick={() => {\n                setActive(1);\n              }}\n            />\n            <Pagination.Prev\n              disabled={active === 1}\n              onClick={() => {\n                setActive(active - 1);\n              }}\n            />\n            {paginated.map((item, index) => {\n              return (\n                <Pagination.Item\n                  key={index}\n                  active={index + 1 === active}\n                  onClick={() => {\n                    setActive(index + 1);\n                  }}\n                >\n                  {index + 1}\n                </Pagination.Item>\n              );\n            })}\n            <Pagination.Next\n              disabled={active === paginated.length}\n              onClick={() => {\n                setActive(active + 1);\n              }}\n            />\n            <Pagination.Last\n              disabled={active === paginated.length}\n              onClick={() => {\n                setActive(paginated.length);\n              }}\n            />\n          </Pagination>\n\n          <ListGroup>\n            {paginated[active - 1].map((item, index) => {\n              return (\n                <Link href={`/${item.name}`} key={index}>\n                  <a>\n                    <ListGroup.Item key={index}>{item.name}</ListGroup.Item>\n                  </a>\n                </Link>\n              );\n            })}\n          </ListGroup>\n        </Container>\n      </main>\n    </div>\n  );\n}\n\nexport async function getStaticProps(context) {\n  const Pokedex = require(\"pokedex-promise-v2\");\n  const P = new Pokedex();\n  const list = await P.getPokemonsList();\n  // console.log(list);\n\n  let pathList = [];\n\n  const getList = async () => {\n    const list = await P.getPokemonsList();\n\n    for (var i = 0, len = list.length; i < len; i++) {\n      // someFn(arr[i]);\n\n      pathList.push(list[i].name);\n    }\n  };\n\n  getList();\n\n  console.log(pathList);\n\n  const chunk = (arr, size) => {\n    return Array.from({ length: Math.ceil(arr.length / size) }, (v, i) =>\n      arr.slice(i * size, i * size + size)\n    );\n  };\n\n  const paginated = chunk(list.results, 250);\n  return { props: { paginated } };\n}\n\n// export async function getServerSideProps() {\n//   const Pokedex = require('pokedex-promise-v2')\n//   const P = new Pokedex()\n//   const list = await P.getPokemonsList()\n//   const chunk = (arr, size) => {\n//     return Array.from({ length: Math.ceil(arr.length / size) }, (v, i) =>\n//       arr.slice(i * size, i * size + size)\n//     )\n//   }\n\n//   const paginated = chunk(list.results, 250)\n//   return { props: { paginated } }\n// }\n"]},"metadata":{},"sourceType":"module"}